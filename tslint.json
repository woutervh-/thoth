{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": [
            true,
            "match-declaration-order"
        ],
        "no-any": true,
        "quotemark": [
            true,
            "single"
        ],
        "array-type": [
            true,
            "array"
        ],
        "max-line-length": false,
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "no-shadowed-variable": false,
        "interface-name": false
    },
    "rulesDirectory": []
}